<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shara.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DevotionalActivity"
            android:label="Devotional Soongs" />
        <activity
            android:name=".ClassicalActivity"
            android:label="Classical Songs" />
        <activity
            android:name=".ArtActivity"
            android:label="Art Songs" />
        <activity android:name=".PopularActivity"></activity>
    </application>

</manifest>